{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AuthLoginRequest",
  "type": "object",
  "required": ["email", "password"],
  "properties": {
    "email": {
      "type": "string",
      "format": "email",
      "minLength": 5,
      "maxLength": 254
    },
    "password": {
      "type": "string",
      "minLength": 8,
      "maxLength": 128
    }
  },
  "additionalProperties": false
}